{
  "name": "default-personal",
  "description": "Personal development with GitHub",
  "extends": "default",
  "mcp_servers": [
    {
      "name": "filesystem",
      "enabled": true
    },
    {
      "name": "git",
      "enabled": true
    },
    {
      "name": "github",
      "enabled": true,
      "config": {
        "owner": "your-github-username"
      }
    },
    {
      "name": "duckduckgo",
      "enabled": true
    },
    {
      "name": "memory-bank",
      "enabled": true
    },
    {
      "name": "memory",
      "enabled": true,
      "config": {
        "MEMORY_FILE_PATH": "~/.aidev/shared_memory.jsonl"
      }
    }
  ],
  "environment": {
    "GITHUB_TOKEN": "${GITHUB_TOKEN}",
    "MEMORY_BANK_ROOT": "${HOME}/.aidev/memory-banks/personal"
  }
}
