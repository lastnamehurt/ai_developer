{
  "name": "ml-engineer",
  "description": "Machine learning and AI development",
  "extends": "data",
  "mcp_servers": [
    {
      "name": "filesystem",
      "enabled": true
    },
    {
      "name": "git",
      "enabled": true
    },
    {
      "name": "jupyter",
      "enabled": true,
      "config": {
        "kernels": [
          "python3",
          "julia"
        ]
      }
    },
    {
      "name": "postgres",
      "enabled": true
    },
    {
      "name": "s3",
      "enabled": true,
      "config": {
        "buckets": [
          "ml-datasets",
          "model-artifacts"
        ]
      }
    },
    {
      "name": "mlflow",
      "enabled": true
    },
    {
      "name": "memory",
      "enabled": true,
      "config": {
        "MEMORY_FILE_PATH": "~/.aidev/shared_memory.jsonl"
      }
    }
  ],
  "environment": {
    "GITHUB_TOKEN": "${GITHUB_TOKEN}",
    "AWS_ACCESS_KEY_ID": "${AWS_ACCESS_KEY_ID}",
    "AWS_SECRET_ACCESS_KEY": "${AWS_SECRET_ACCESS_KEY}",
    "MLFLOW_TRACKING_URI": "${MLFLOW_TRACKING_URI}",
    "CUDA_VISIBLE_DEVICES": "0"
  }
}
